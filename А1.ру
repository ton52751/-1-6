
№1

import math

x = 0.1
z = 2

result = 4**(2**3*x) + x**(z**3) - math.exp(x**3/3) + 0.4

print(result)

№2

import math

x = 3
result = math.pow(math.sqrt(3 + 2 * math.cos(math.pow(x, 2))), 3) + math.pow(math.fabs(math.log(x / 2)), 3)
print(result)

№3

import math

x = 2
result = x * ((x - 1) / (x + 4)) + math.sqrt(math.exp(x**2) - 1)

print(result)
№4

import math

x = 0.2
y = 1
result = 1.6 * ((math.cos(x)**3 + math.atan(3*x)) / (4*x*y))

print(result)
